{
  "runtime": "Net60",
  "documentGenerator": {
    "fromDocument": {
      "url": "https://chuck-wars-api.azurewebsites.net/swagger/v1/swagger.json"
    }
  },
  "codeGenerators": {
    "openApiToCSharpClient": {
      "className": "{controller}Client",
      "output": "ChuckWarsClient.g.cs",
      "namespace": "ChuckWarsUI.Integration.ChuckWarsApi",
      "generateClientInterfaces": true,
      "injectHttpClient": false,
      "disposeHttpClient": true
    }
  }
}